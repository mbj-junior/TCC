    if (etapaAtual === formularios.length - 1) {\n      coletarDados(dadosColetados);\n    }\n  });\n\n  function coletarDados(dados) {\n    setDados({ ...dadosColetados, ...dados });\n    proximo();\n  }\n\n  function getDados() {\n    return dadosColetados;\n  }\n\n  function proximo() {\n    setEtapaAtual(etapaAtual + 1);\n  }\n\n  const formularios = [\n    <DadosUsuario aoEnviar={coletarDados} />,\n    <DadosPessoais aoEnviar={coletarDados} getDados={getDados} />,\n    <Confirmacao />,\n  ];\n\n  return (\n    <>\n      <Stepper activeStep={etapaAtual}>\n        <Step>\n          <StepLabel>Login</StepLabel>\n        </Step>\n        <Step>\n          <StepLabel>Pessoal</StepLabel>\n        </Step>\n        <Step>\n          <StepLabel>Confirmação</StepLabel>\n        </Step>\n      </Stepper>\n\n      {formularios[etapaAtual]}\n    </>\n  );\n}\n\nexport default FormularioCadastro;\n","/home/marcio/Documentos/IFSP/TCC/oumeajuda/src/component/formularioLogin/FormularioLogin.jsx",[],"/home/marcio/Documentos/IFSP/TCC/oumeajuda/src/component/muralPedidos/Pedido.jsx",[],"/home/marcio/Documentos/IFSP/TCC/oumeajuda/src/component/formularioAjuda/ListaDePedidos/index.js",[],"/home/marcio/Documentos/IFSP/TCC/oumeajuda/src/component/formularioAjuda/FormularioPedido/index.js",[],"/home/marcio/Documentos/IFSP/TCC/oumeajuda/src/component/formularioCadastro/DadosUsuario.jsx",[],"/home/marcio/Documentos/IFSP/TCC/oumeajuda/src/component/formularioLogin/UsuarioLogadoPage.jsx",["121"],"import { Button, Typography } from \"@material-ui/core\";\n\nimport React from \"react\";\nimport { useCookies } from \"react-cookie\";\n\nexport default function UsuarioLogadoPage() {\n  const [cookies, removeCookie] = useCookies([\"token\"]);\n\n  function handleRemoveCookie() {\n    removeCookie(\"token\");\n  }\n\n  return (\n    <>\n      <br></br>\n      <Typography variant=\"h4\" align=\"center\">\n        Seja bem vinda(o).\n      </Typography>\n      <Typography variant=\"h5\" align=\"center\">\n        Você já está Conectada(o).\n      </Typography>\n      <br></br>\n      <br></br>\n      <br></br>\n      <Button\n        type=\"button\"\n        variant=\"contained\"\n        color=\"primary\"\n        onClick={handleRemoveCookie}\n        fullWidth\n      >\n        SAIR\n      </Button>\n    </>\n  );\n}\n","/home/marcio/Documentos/IFSP/TCC/oumeajuda/src/component/muralPedidos/ContatoPopover.jsx",[],"/home/marcio/Documentos/IFSP/TCC/oumeajuda/src/component/formularioAjuda/ListaDePedidos/ListaDePedidos.jsx",[],"/home/marcio/Documentos/IFSP/TCC/oumeajuda/src/component/formularioAjuda/FormularioPedido/FormularioPedido.jsx",[],"/home/marcio/Documentos/IFSP/TCC/oumeajuda/src/hooks/useErros.js",[],"/home/marcio/Documentos/IFSP/TCC/oumeajuda/src/component/formularioAjuda/CardPedido/index.js",[],"/home/marcio/Documentos/IFSP/TCC/oumeajuda/src/component/formularioAjuda/CardPedido/CardPedido.jsx",[],"/home/marcio/Documentos/IFSP/TCC/oumeajuda/src/component/formularioLogin/DadosUsuarioLogin.jsx",["122"],"import { Button, TextField } from \"@material-ui/core\";\nimport React, { useContext, useState } from \"react\";\nimport { Typography } from \"@material-ui/core\";\nimport ValidacoesCadastro from \"../../contexts/ValidacoesCadastro\";\nimport { useCookies } from \"react-cookie\";\nimport useErros from \"../../hooks/useErros\";\n\nexport default function DadosUsuarioLogin() {\n  const [email, setEmail] = useState(\"\");\n  const [senha, setSenha] = useState(\"\");\n  const validacoes = useContext(ValidacoesCadastro);\n  const [erros, validarCampos, possoEnviar] = useErros(validacoes);\n  const [cookies, setCookie] = useCookies([\"token\"]);\n\n  function handleCookie(token) {\n    setCookie(\"token\", token, {\n      path: \"/\",\n    });\n  }\n\n  const onSubmit = (event) => {\n    event.preventDefault();\n\n    const body = {\n      email: event.target[0].defaultValue,\n      senha: event.target[2].defaultValue,\n    };\n\n    logar(body);\n  };\n\n  const logar = async (body) => {\n    return await fetch(\"http://localhost:7000/login\", {\n      credentials: \"same-origin\",\n      method: \"POST\",\n      headers: {\n        'Accept': 'application/json',\n        'Content-Type': 'application/json'\n      },\n      body: JSON.stringify(body),\n    })\n      .then((resp) => {\n        if (resp.status === 200) {\n          return resp.json();\n        } else if (resp.status === 500) {\n          const error = new Error(resp.error);\n          alert(\"Erro de aplicação. Tente novamente mais tarde.\");\n          return error;\n        } else {\n          const error = new Error(resp.error);\n          alert(\"Email ou senha incorretos. Tente novamente.\");\n          return error;\n        }\n      })\n      .then((json) => {\n        if (json.token) {\n          handleCookie(json.token);\n        }\n\n        return json;\n      });\n  };\n\n  return (\n    <>\n      <br></br>\n      <Typography variant=\"h4\" component=\"h1\" align=\"center\">\n        Acesse sua conta\n      </Typography>\n      <br></br>\n      <form\n        onSubmit={(event) => {\n          event.preventDefault();\n          if (possoEnviar()) {\n            onSubmit(event);\n          }\n        }}\n      >\n        <TextField\n          value={email}\n          onChange={(event) => {\n            setEmail(event.target.value);\n          }}\n          id=\"email\"\n          label=\"e-mail\"\n          type=\"email\"\n          required\n          variant=\"outlined\"\n          margin=\"normal\"\n          fullWidth\n        />\n        <TextField\n          value={senha}\n          onChange={(event) => {\n            setSenha(event.target.value);\n          }}\n          onBlur={validarCampos}\n          error={!erros.senha.valido}\n          helperText={erros.senha.texto}\n          id=\"senha\"\n          label=\"senha\"\n          name=\"senha\"\n          required\n          type=\"password\"\n          variant=\"outlined\"\n          margin=\"normal\"\n          fullWidth\n        />\n\n        <Button type=\"submit\" variant=\"contained\" color=\"primary\" fullWidth>\n          ENTRAR\n        </Button>\n      </form>\n    </>\n  );\n}\n","/home/marcio/Documentos/IFSP/TCC/oumeajuda/src/component/formularioCadastro/DadosPessoais.jsx",[],"/home/marcio/Documentos/IFSP/TCC/oumeajuda/src/component/formularioCadastro/Confirmacao.jsx",[],{"ruleId":"123","replacedBy":"124"},{"ruleId":"125","replacedBy":"126"},{"ruleId":"127","severity":1,"message":"128","line":1,"column":36,"nodeType":"129","messageId":"130","endLine":1,"endColumn":46},{"ruleId":"127","severity":1,"message":"131","line":7,"column":10,"nodeType":"129","messageId":"130","endLine":7,"endColumn":17},{"ruleId":"127","severity":1,"message":"131","line":13,"column":10,"nodeType":"129","messageId":"130","endLine":13,"endColumn":17},"no-native-reassign",["132"],"no-negated-in-lhs",["133"],"no-unused-vars","'Typography' is defined but never used.","Identifier","unusedVar","'cookies' is assigned a value but never used.","no-global-assign","no-unsafe-negation"]